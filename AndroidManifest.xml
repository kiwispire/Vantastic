<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="nz.co.iknowtech.vantastic"
      android:versionName="1.2.4" android:versionCode="19">

	<uses-sdk android:minSdkVersion="4"/>
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application android:icon="@drawable/ic_launcher" 
    			 android:label="@string/app_name" 
    			 android:theme="@android:style/Theme.NoTitleBar"
    			 android:debuggable="false">
    	<uses-library android:name="com.google.android.maps"/>
 
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" /> 
            </intent-filter>
            <intent-filter>
            	<action android:name="android.intent.action.VIEW" />
            	<category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
            	<action android:name="android.intent.action.SEND" />
            	<category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".InterOfficeTable">
        </activity>
        <activity android:name=".Airport_table">
        </activity>
        <activity android:name=".Auckland">
        </activity>
        <activity android:name=".Wellington">
        </activity>
        <activity android:name=".LocateActivity">
        </activity>
    </application>
</manifest> 
